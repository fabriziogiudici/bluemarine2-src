<QUERIES>
    <AUTH>
        <CLIENT>@CLIENT_ID@</CLIENT>
        <USER>@USER_ID@</USER>
    </AUTH>
    <LANG>eng</LANG>
    <COUNTRY>usa</COUNTRY>
    <QUERY CMD="ALBUM_TOC">
        <MODE>SINGLE_BEST_COVER</MODE>
        <TOC>
            <OFFSETS>@OFFSETS@</OFFSETS>
        </TOC>
    </QUERY>
</QUERIES>
